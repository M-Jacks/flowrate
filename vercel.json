
{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/login", "dest": "/server.js" }, 
    { "src": "/tests", "dest": "/server.js" }, 
    { "src": "/results", "dest": "/server.js" }, 
    { "src": "/products", "dest": "/server.js" }, 
    { "src": "/reports", "dest": "/server.js" }, 
    { "src": "/comparison", "dest": "/server.js" }, 
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
    